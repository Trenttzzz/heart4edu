{
  "name": "heart4edu-system-monitor",
  "version": "1.0.0",
  "description": "Real-time CPR Guidance System - Landing Page",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "dev:backend": "cd ../heart4edu-backend && uvicorn app:app --host 0.0.0.0 --port 8000 --reload",
    "dev:fullstack": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "start:backend": "cd ../heart4edu-backend && python -m uvicorn app:app --host 0.0.0.0 --port 8000"
  },
  "dependencies": {
    "@types/node": "^24.3.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.9.2",
    "vite": "^4.5.0"
  },
  "keywords": [
    "cpr",
    "medical",
    "monitoring",
    "real-time",
    "healthcare",
    "education"
  ],
  "author": "Heart4Edu Team",
  "license": "MIT"
}
